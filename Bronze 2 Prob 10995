package Java;

import java.util.*;
import java.io.*;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int input;
		input = sc.nextInt();
		
		for(int i=0; i<input; i++) {
			if((i%2) == 0) {
				for(int j=0; j<input; j++) 
					System.out.printf("* ");
				System.out.println();
			}
			else {
				for(int j=0; j<input; j++)
					System.out.printf(" *");
				System.out.println();
			}
		}
		
		sc.close();
	}
}
